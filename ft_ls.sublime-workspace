n	}\n	else\n	{\n		result = b;\n		result->next = merge_r(a, b->next);\n	}\n	return (result);\n}\n\n\nvoid	split(ll_list *source, ll_list **front, ll_list **back)\n{\n	ll_list	*fast;\n	ll_list *slow;\n	\n	if (!source || !source->next)\n	{\n		*front = source;\n		*back = NULL;\n	}\n	else\n	{\n		slow = source;\n		fast = source->next;\n		while (fast)\n		{\n			fast = fast->next;\n			if (fast)\n			{\n				slow = slow->next;\n				fast = fast->next;\n			}\n		}\n		*front = source;\n		*back = slow->next;\n		slow->next = NULL;\n	}	\n}\n",
